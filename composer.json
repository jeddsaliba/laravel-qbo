{
    "name": "pns/laravel-qbo",
    "description": "This is a Laravel package for integrating QuickBooks Online API.",
    "type": "library",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "quickbooks/v3-php-sdk": "^6.0",
        "ext-mbstring": "*",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0 || ^6.0 || ^7.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pns\\LaravelQbo\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jedd Saliba",
            "email": "jeddsaliba@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Pns\\LaravelQbo\\Providers\\LaravelQboServiceProvider"
            ]
        }
    }
}
